class Solution {
    static int count = 0;

    public static void zero(int num1, int num2) {
        if (num1 == 0 || num2 == 0) return;
        if (num1 > num2) {
            num1 = num1 - num2;
        } else {
            num2 = num2 - num1;
        }
        count++;
        zero(num1, num2);
    }

    public int countOperations(int num1, int num2) {
        count = 0;
        zero(num1, num2);
        return count;
    }
}
